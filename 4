type Query {
  # Fetch scatter plot data by protocol
  getScatterPlotDataByProtocol(protocol: String!): [ScatterPlotData!]!

  # Filter inventory data by protocol
  filterDataByProtocol(protocol: String!): [UCSPAlarmInventory!]!

  # Get all available protocols
  availableProtocols: [String!]!
}

# Data type for scatter plot visualization
type ScatterPlotData {
  collectionName: String!
  date: String!
  value: Int!
}

# Data type for inventory records
type UCSPAlarmInventory {
  id: ID!
  collectionName: String!
  deviceName: String!
  deviceIp: String!
  ucgSource: String!
  vendor: String!
}