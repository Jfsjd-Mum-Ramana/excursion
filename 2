type ScatterPlotData {
  collectionName: String
  lastUpdated: String
  yValue: Long
}

type Query {
  # Fetch scatter plot data by protocol and data type
  getScatterPlotDataByProtocol(protocol: String!, dataType: String!): [ScatterPlotData]

  # Fetch available protocols
  getAvailableProtocols: [String]

  # Filter scatter plot data by protocol
  filterDataByProtocol(protocol: String!, dataType: String!): [ScatterPlotData]
}