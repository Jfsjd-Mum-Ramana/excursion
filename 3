# Define the ScatterPlotData type which represents the data for scatter plot
type ScatterPlotData {
  collectionName: String
  protocol: String
  collectionDate: String
  yValue: Float
}

# Define the Query type that contains all the queries for our GraphQL API
type Query {

  # Query to fetch scatter plot data based on protocol and date range
  getScatterPlotDataByProtocolAndDateRange(
    protocol: String!, 
    startDate: String!, 
    endDate: String!
  ): [ScatterPlotData]

  # Query to fetch scatter plot data filtered by protocol
  getScatterPlotDataByProtocol(
    protocol: String!
  ): [ScatterPlotData]

  # Query to fetch all available protocols
  getAvailableProtocols: [String]
}